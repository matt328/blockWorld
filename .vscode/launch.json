  "version": "0.2.0",
  "configurations": [
    {
      "type": "java",
      "name": "Launch Current File",
      "request": "launch",
      "mainClass": "${file}"
    },
    {
      "type": "java",
      "name": "Launch EditorApplication",
      "request": "launch",
      "mainClass": "org.blockworldeditor.main.EditorApplication",
      "projectName": "editor"
    },
    {
      "type": "java",
      "name": "Launch FunctionEditor",
      "request": "launch",
      "mainClass": "org.blockworldeditor.main.FunctionEditor",
      "projectName": "editor"
    },
    {
      "type": "java",
      "name": "Launch ColorScan",
      "request": "launch",
      "mainClass": "org.blockworldeditor.ui.ColorScan",
      "projectName": "editor"
    },
    {
      "type": "java",
      "name": "Launch BlockworldApplication",
      "request": "launch",
      "mainClass": "org.blockworld.main.BlockworldApplication",
      "projectName": "game"
    },
    {
      "type": "java",
      "name": "Launch FunctionLoader",
      "request": "launch",
      "mainClass": "org.blockworldshared.math.functions.loader.FunctionLoader",
      "projectName": "shared"
    }
  ]
}
